@model SchoolManagementSystem.Models.StudentFee

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>StudentFee</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.FeeType, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="form-group">
                @Html.DropDownList("ddlID", new SelectList(ViewBag.stndrd as System.Collections.IEnumerable, "Id", "FeeType", "feeId"), htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Id, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.RegistratiomFee, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="form-group">
                @Html.TextBox("RegText")
            </div>
            </div>
        <div class="form-group">
            @Html.LabelFor(model => model.AdmissionFee, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="form-group">
                @Html.TextBox("AdmText")
            </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>
        </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
